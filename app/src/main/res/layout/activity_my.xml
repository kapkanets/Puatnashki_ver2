<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MyActivity">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:padding="0dp"
        android:src="@drawable/p1"
        android:layout_marginLeft="30dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/imageButton1"
        android:padding="0dp"
        android:layout_margin="0dp"
        android:src="@drawable/p2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton3"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/imageButton2"
        android:padding="0dp"
        android:layout_margin="0dp"
        android:src="@drawable/p3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton4"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/imageButton3"
        android:src="@drawable/p4"
        android:layout_margin="0dp"
        android:padding="0dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton5"
        android:layout_below="@+id/imageButton1"
        android:layout_alignEnd="@+id/imageButton1"
        android:layout_margin="0dp"
        android:padding="0dp"
        android:src="@drawable/p5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton6"
        android:layout_alignBottom="@+id/imageButton5"
        android:layout_toEndOf="@+id/imageButton1"
        android:layout_margin="0dp"
        android:padding="0dp"
        android:src="@drawable/p6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton7"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_toEndOf="@+id/imageButton6"
        android:layout_margin="0dp"
        android:padding="0dp"
        android:src="@drawable/p7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton8"
        android:padding="0dp"
        android:src="@drawable/p8"
        android:layout_toEndOf="@+id/imageButton2"
        android:layout_below="@+id/imageButton4"
        android:layout_alignStart="@+id/imageButton4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton9"
        android:layout_below="@+id/imageButton5"
        android:layout_alignParentStart="true"
        android:padding="0dp"
        android:src="@drawable/p9"
        android:layout_marginLeft="30dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton10"
        android:layout_alignTop="@+id/imageButton9"
        android:layout_toEndOf="@+id/imageButton9"
        android:layout_margin="0dp"
        android:padding="0dp"
        android:src="@drawable/p10" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:layout_alignTop="@+id/imageButton10"
        android:layout_toEndOf="@+id/imageButton10"
        android:layout_margin="0dp"
        android:padding="0dp"
        android:src="@drawable/p11" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton12"
        android:layout_alignTop="@+id/imageButton11"
        android:layout_toEndOf="@+id/imageButton11"
        android:layout_margin="0dp"
        android:padding="0dp"
        android:src="@drawable/p12" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:layout_below="@+id/imageButton9"
        android:layout_alignParentStart="true"
        android:padding="0dp"
        android:src="@drawable/p13"
        android:layout_marginLeft="30dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton14"
        android:layout_below="@+id/imageButton9"
        android:layout_toEndOf="@+id/imageButton13"
        android:padding="0dp"
        android:layout_margin="0dp"
        android:src="@drawable/p14" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton15"
        android:layout_below="@+id/imageButton9"
        android:layout_toEndOf="@+id/imageButton14"
        android:padding="0dp"
        android:layout_margin="0dp"
        android:src="@drawable/p15" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton16"
        android:layout_below="@+id/imageButton10"
        android:layout_toEndOf="@+id/imageButton15"
        android:padding="0dp"
        android:layout_margin="0dp"
        android:src="@drawable/p16" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonStart"
        android:layout_below="@+id/imageButton13"
        android:layout_toStartOf="@+id/imageButton15"
        android:layout_marginTop="42dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="30dp"
        android:padding="0dp"
        android:src="@drawable/start_logo" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageDog"
        android:layout_toEndOf="@+id/imageButtonStart"
        android:src="@drawable/dog"
        android:layout_below="@+id/imageButton15"
        android:layout_marginLeft="15dp"
        android:padding="0dp"
        android:layout_marginTop="25dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageCat"
        android:layout_below="@+id/imageButton16"
        android:layout_toEndOf="@+id/imageDog"
        android:src="@drawable/cat"
        android:padding="0dp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="15dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageAlex"
        android:layout_below="@+id/imageDog"
        android:layout_toEndOf="@+id/imageButtonStart"
        android:src="@drawable/alex"
        android:layout_marginLeft="15dp"
        android:padding="0dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="15dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageGirl"
        android:layout_alignTop="@+id/imageAlex"
        android:layout_alignStart="@+id/imageCat"
        android:src="@drawable/girll"
        android:padding="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textRecord"
        android:layout_below="@+id/imageAlex"
        android:layout_alignStart="@+id/imageButton13"
        android:layout_alignEnd="@+id/imageButtonStart" />

</RelativeLayout>
